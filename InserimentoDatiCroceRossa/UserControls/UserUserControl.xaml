<UserControl x:Class="InserimentoDatiCroceRossa.UserControls.UserUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:images="clr-namespace:InserimentoDatiCroceRossa.Objects"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <ToolBar Grid.Row="0">
            <Button x:Name="SaveButton" Click="SaveButton_Click">
                <Image Source="{images:Images ImageName=add}"/>
            </Button>
            <Button x:Name="ClearButton" Click="ClearButton_Click">
                <Image Source="{images:Images ImageName=add}"/>
            </Button>
        </ToolBar>
    </Grid>
</UserControl>
